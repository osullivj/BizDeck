<?xml version="1.0" encoding="utf-8" ?>
<dialog width="200" height="300" title="Delete button">
    <vbox width="100%" height="100%">
        <listview id="bd_del_btn_listview" width="100%" height="100%">
            <item-renderer layoutName="horizontal" width="100%" height="50">
                <label width="50%" height="50" id="bd_del_btn_name" verticalAlign="center" />
                <label width="50%" height="50" id="bd_del_btn_type" verticalAlign="center" />
                <image width="50" height="50" id="bd_del_btn_icon" />	
            </item-renderer>
        </listview>
	</vbox>
</dialog>